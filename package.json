{
  "name": "js-ds-crdt",
  "version": "0.0.10",
  "main": "dist/lib/index.js",
  "license": "MIT",
  "type": "module",
  "author": "dozyio",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo",
    "!.gitignore",
    "!.prettierignore"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "build": "aegir build",
    "test": "vitest run --hideSkippedTests --allowOnly",
    "test:single": "vitest replicate --hideSkippedTests --allowOnly ",
    "test:bench": "vitest bench",
    "test:watch": "vitest",
    "clean": "aegir clean",
    "generate": "protons ./src/pb/*.proto",
    "lint": "aegir lint --fix",
    "release": "aegir release",
    "release:rc": "aegir release-rc",
    "unused": "npx knip"
  },
  "devDependencies": {
    "@libp2p/tcp": "^9.1.5",
    "@types/node": "^22.4.0",
    "aegir": "^44.1.1",
    "blockstore-core": "^5.0.0",
    "delay": "^6.0.0",
    "eslint-plugin-n": "^17.10.2",
    "prettier": "^3.3.3",
    "protobufjs": "^7.3.2",
    "protons": "^7.6.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^13.1.1",
    "@chainsafe/libp2p-noise": "^15.1.1",
    "@chainsafe/libp2p-yamux": "^6.0.2",
    "@ipld/dag-pb": "^4.1.2",
    "@libp2p/identify": "^2.1.5",
    "@libp2p/interface": "^1.7.0",
    "@libp2p/logger": "^4.0.20",
    "@multiformats/multiaddr": "^12.3.0",
    "async-mutex": "^0.5.0",
    "datastore-core": "^10.0.0",
    "helia": "^4.2.5",
    "interface-blockstore": "^5.3.0",
    "interface-datastore": "^8.3.0",
    "interface-store": "^6.0.0",
    "libp2p": "^1.9.2",
    "multiformats": "^13.2.2",
    "protons-runtime": "^5.5.0",
    "uint8arraylist": "^2.4.8",
    "uint8arrays": "^5.1.0",
    "weald": "^1.0.2"
  }
